{
  "name": "node-langchain",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/node-langchain/src",
  "projectType": "library",
  "tags": ["tech:node"],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/node-langchain",
        "main": "libs/node-langchain/src/index.ts",
        "tsConfig": "libs/node-langchain/tsconfig.lib.json",
        "assets": ["libs/node-langchain/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/node-langchain/jest.config.ts"
      }
    }
  }
}
