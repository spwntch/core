{
  "name": "@spwntch/core",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "rm:dist": "find . -name 'dist' -type d -prune -exec rm -rf '{}' +",
    "rm:node-modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "rm:tmp": "rm -rf tmp",
    "rm:lockfile": "rm pnpm-lock.yaml",
    "clean": "pnpm run rm:dist && pnpm run rm:node-modules && pnpm run rm:tmp && pnpm run rm:lockfile",
    "build": "pnpm dlx nx run-many -t build",
    "release": "pnpm dlx nx release",
    "publish": "pnpm dlx nx release publish",
    "storybook": "pnpm dlx nx run react-storybook:storybook",
    "webp": "tools/scripts/webp.sh ",
    "registry": "pnpm dlx nx local-registry",
    "spawn-workspace": "pnpm dlx nx run spawn-workspace",
    "add-shad": "pnpm dlx nx run react-components:add-shad",
    "ci": "pnpm dlx nx run-many -t lint test build build-storybook"
  },
  "private": true,
  "dependencies": {
    "next": "14.0.4",
    "react": "18.2.0",
